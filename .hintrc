{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-lang-valid": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "height: fit-content",
          "mask-size"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  }
}